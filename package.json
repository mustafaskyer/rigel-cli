{
  "name": "rigel-cli",
  "bin": {
    "rigel": "lib/bin/rigel.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mustafaskyer/rigel-cli.git"
  },
  "scripts": {
    "test": "node_modules/.bin/mocha",
