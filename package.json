    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.2.0",
    "mocha": "^9.1.2",
    "sinon": "^8.1.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
