    "fuzzy": "^0.1.3",
    "handlebars": "^4.7.6",
    "inquirer": "^7.0.3",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "inquirer-checkbox-plus-prompt": "^1.0.1",
    "lodash": "^4.17.15",
    "lower-case-first": "^2.0.1",
    "minimist": "^1.2.5",
